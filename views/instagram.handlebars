  <title>Instagram Feed</title>
  <style>
    .instagram-container {
      display: flex;
      flex-wrap: wrap;
      justify-content: center;
      gap: 1rem;
      margin-top: 50px;
    }

    .instagram-item {
      width: 200px;
      text-align: center;
    }

    .instagram-image {
      width: 100%;
      height: 200px;
      object-fit: cover;
    }

    .instagram-caption {
      margin-top: 0.5rem;
      font-size: 1.2rem;
      color: #dfdfdf;
      word-wrap: break-word;
    }
    h1 {
      text-align: center;
      color: #d70000;
      font-size: 5rem;
      border-bottom: 2px solid #8d0000; /* Add a line below the text */
      padding-bottom: 10px; /* Add space between the text and the line */
    }
  </style>
  <h1>Live Instagram Feed</h1>
<body>
  <div class="instagram-container">
    {{#each mediaData}}
      <div class="instagram-item">
        <img src="{{this.media_url}}" alt="{{this.caption}}" class="instagram-image">
        <p class="instagram-caption">{{this.caption}}</p>
      </div>
    {{/each}}
  </div>
